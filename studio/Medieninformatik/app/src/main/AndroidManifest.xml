<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ifi.lmu.com.handmeasurementstudy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Tapping"
            android:label="@string/title_activity_tapping" />
        <activity
            android:name=".Swiping"
            android:label="@string/title_activity_swiping" />
        <activity
            android:name=".Scrolling"
            android:label="@string/title_activity_scrolling" />
        <activity
            android:name=".Zooming"
            android:label="@string/title_activity_zooming" />
        <activity android:name=".Tablet" />
        <activity android:name=".TabletActivity"></activity>
    </application>

</manifest>